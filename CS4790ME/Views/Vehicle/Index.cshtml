@model IEnumerable<Vehicle>
@using CS4790ME.Models
@using MVCGrid.Web
@{
    ViewBag.Title = "Index";
}


<div class="center-block">
    <h2>60Hz Input History</h2>
</div>

<div class="panel panel-default">
    <div class="panel-body">
        @Html.Partial("_MVCGridToolbar", new MVCGridToolbarModel()
   {
       MVCGridName = "Vehicles",
       PageSize = true,
       ColumnVisibility = true,
       Export = true,
       GlobalSearch = true
   })
        @Html.MVCGrid("Vehicles")
    </div>
</div>